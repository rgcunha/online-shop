{
  "name": "online-shop",
  "version": "0.0.1",
  "description": "Backend system for an online-shop",
  "main": "dist/index.js",
  "scripts": {
    "clear:dist": "rm -rf dist",
    "clear:temp": "rm -rf temp",
    "prebuild": "npm run clear:dist",
    "build": "tsc",
    "db:seeds": "ts-node ./bin/start-db.ts",
    "docs:dev": "redoc-cli serve docs/endpoints.yaml --watch",
    "lint": "eslint src --ext .js,.ts --max-warnings=0",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "prod": "node ./dist/src/Server.js",
    "start": "ts-node src/server.ts",
    "test": "ENVIRONMENT=development npm run test:integration && npm run test:unit",
    "test:unit": "jest src/ --coverage",
    "test:integration": "jest test/integration --coverage",
    "prei18nLint": "npm run clear:temp && tsc --outDir temp",
    "i18nLint": "i18nLint",
    "i18nLint:fix": "i18nLint -f",
    "i18nSync": "i18nSync"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rgcunha/online-shop.git"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/http-errors": "1.6.3",
    "@types/mongoose": "^5.7.32",
    "@types/rosie": "0.0.37",
    "axios": "0.21.1",
    "connect-mongo": "^4.4.1",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-basic-auth": "1.2.0",
    "express-session": "^1.17.1",
    "express-winston": "4.0.3",
    "helmet": "3.22.0",
    "http-errors": "1.8.0",
    "inversify": "5.0.1",
    "inversify-express-utils": "6.3.2",
    "moment": "2.29.1",
    "mongoose": "5.9.25",
    "nodemon": "2.0.4",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "qs": "6.9.4",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.19",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "4.17.6",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "0.0.47",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.22",
    "@types/passport-local": "^1.0.33",
    "@types/source-map-support": "0.5.1",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "3.0.1",
    "@typescript-eslint/parser": "3.0.1",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "i18next": "19.4.5",
    "jest": "26.0.1",
    "jest-extended": "0.11.5",
    "newman": "5.1.0",
    "prettier": "2.0.5",
    "redoc-cli": "0.9.8",
    "rosie": "2.0.1",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "3.9.3"
  },
  "engines": {
    "node": "=14.3.0",
    "npm": "=6.14.4"
  }
}
